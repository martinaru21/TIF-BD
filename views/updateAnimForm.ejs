<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Update ANIMACION</title>
  </head>
  <body>
    <h2>Update Data in ANIMACION Table</h2>
    <form action="/updates/submitUpdatedAnim" method="post">
      <label for="id">ID:</label>
      <input
        type="text"
        name="id"
        value="<%= animData.id_arte %>"
        readonly
      /><br />

      <label for="nombre">Name:</label>
      <input
        type="text"
        name="nombre"
        value="<%= animData.nombre_arte %>"
        required
      /><br />

      <label for="direccion">Address:</label>
      <input
        type="text"
        name="direccion"
        value="<%= animData.direccion_NAS %>"
        required
      /><br />

      <label for="grupo">Group:</label>
      <input
        type="text"
        name="grupo"
        value="<%= animData.grupo %>"
        required
      /><br />

      <label for="idAsset">ASSET ID:</label>
      <input
        type="text"
        name="idAsset"
        value="<%= animData.id_escenario %>"
        readonly
      /><br />

      <button type="submit">Update Data</button>
    </form>
  </body>
</html>
